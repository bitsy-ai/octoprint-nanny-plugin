[dash]
base_url = "http://127.0.0.1:9001"

[paths]
os_release = "{{ PRINTNANNY_OS_RELEASE }}"
events_socket = "{{ TMP_DIR }}/events.sock"
license = "{{ PRINTNANNY_LICENSE_JSON }}"
confd = "{{ PRINTNANNY_CONFD }}"

[mqtt]
private_key = "{{ TMP_DIR }}/local/keys/ec_private.pem"
public_key = "{{ TMP_DIR }}/local/keys/ec_public.pem"
ca_certs = ["{{ TMP_DIR }}/local/ca-certificates/gtsltsr.crt"]
fingerprint = "{{ FINGERPRINT }}"
fingerprint_algorithm = "md5"
cipher = "secp256r1"

[keys]
path = "{{ PRINTNANNY_KEYS }}"
force_create = false